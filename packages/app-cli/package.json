{
  "name": "agent",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:cli": "rollup cli.js --format cjs --file bundle.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:cli": "ts-node ./src/cli.ts",
    "start:dev": "DEBUG=true nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "prettier -w .",
    "codegen": "graphql-codegen --config codegen.yml",
    "prestart": "yarn codegen",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@nestjs/common": "^8.4.5",
    "@nestjs/core": "^8.4.5",
    "@nestjs/platform-express": "^8.4.5",
    "@types/ws": "^8.5.4",
    "@willsoto/nestjs-prometheus": "^4.6.0",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.12.0",
    "lodash": "^4.17.21",
    "node-fetch-commonjs": "^3.2.4",
    "puppeteer": "^14.1.1",
    "puppeteer-cluster": "^0.23.0",
    "puppeteer-extra": "^3.2.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "ws": "^8.13.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.5",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-document-nodes": "^2.3.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@nestjs/cli": "^8.2.6",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.11",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
