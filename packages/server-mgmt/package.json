{
  "name": "rich-node-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "clean": "rimraf dist && ts-node prisma/clean.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "seed": "yarn run clean && ts-node prisma/seed.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:migrate:reset": "prisma migrate reset -f",
    "prestart": "yarn codegen",
    "start": "nest start",
    "cli": "ts-node src/cli.ts",
    "prestart:dev": "yarn codegen",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "postinstall": "test -n \"$IGNORE_SCRIPTS\" || yarn prisma generate",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/gateway": "^0.44.1",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^8.0.11",
    "@nestjs/config": "^1.1.5",
    "@nestjs/core": "^8.0.11",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/platform-express": "^8.2.3",
    "@nestjs/serve-static": "^2.2.2",
    "@prisma/client": "^3.6.0",
    "amqplib": "^0.8.0",
    "apollo-server-express": "^2.25.3",
    "class-validator": "^0.13.1",
    "commander": "^8.3.0",
    "dayjs": "^1.10.7",
    "graphql": "^15.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-middleware": "^6.1.13",
    "graphql-scalars": "^1.14.1",
    "lodash": "^4.17.21",
    "prisma": "^3.6.0",
    "puppeteer": "^13.0.0",
    "puppeteer-extra": "^3.2.3",
    "puppeteer-extra-plugin-adblocker": "^2.12.0",
    "puppeteer-extra-plugin-block-resources": "^2.3.0",
    "puppeteer-extra-plugin-stealth": "^2.9.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "type-graphql": "^1.1.1",
    "typegraphql-prisma": "^0.18.2",
    "uuid": "^8.3.2",
    "v8-sandbox": "^2.2.2",
    "xmldom": "^0.6.0",
    "xpath-ts": "^1.3.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-resolvers": "2.4.2",
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.0.11",
    "@types/amqplib": "^0.8.2",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@types/supertest": "^2.0.11",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
