{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "generate-verticals-data": "npx tsc generate-verticals-data.ts --outDir build/ && node build/generate-verticals-data.js",
    "prebuild": "yarn codegen",
    "prestart:feedless": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh feedless false",
    "start:feedless": "ng serve",
    "prestart:rss-proxy": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh rssProxy false",
    "start:rss-proxy": "ng serve",
    "prestart:page-change-tracker": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh pageChangeTracker false",
    "start:page-change-tracker": "ng serve",
    "prestart:untold-notes": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh untoldNotes true",
    "start:untold-notes": "ng serve",
    "prestart:reader": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh reader false",
    "start:reader": "ng serve",
    "prestart:upcoming": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh upcoming false",
    "start:upcoming": "ng serve",
    "start:upcoming:prod": "ng serve -c production",
    "prestart:visual-diff": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh visualDiff false",
    "start:visual-diff": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -c production && yarn generate-verticals-data",
    "test": "jest",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:watch": "jest --watch",
    "lint": "yarn prettier --write --log-level=warn .",
    "outdated": "upgrade-interactive --latest",
    "codegen": "graphql-codegen --config codegen.yml",
    "clean": "rm -rf www build coverage .angular .gradle all-verticals.json src/generated"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.3",
    "@angular/cdk": "^20.2.7",
    "@angular/common": "20.3.3",
    "@angular/core": "20.3.3",
    "@angular/forms": "20.3.3",
    "@angular/platform-browser": "20.3.3",
    "@angular/router": "20.3.3",
    "@angular/service-worker": "20.3.3",
    "@apollo/client": "3.11.9",
    "@codemirror/autocomplete": "^6.12.0",
    "@codemirror/commands": "^6.3.3",
    "@codemirror/language": "^6.10.0",
    "@codemirror/search": "^6.5.5",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.23.0",
    "@ionic/angular": "8.7.5",
    "@justinribeiro/lite-youtube": "1.5.0",
    "@ngx-translate/core": "^16.0.3",
    "@ngx-translate/http-loader": "^16.0.0",
    "d3-scale": "4.0.2",
    "dayjs": "1.11.13",
    "dexie": "^4.2.1",
    "dexie-observable": "^4.0.1-beta.13",
    "flexsearch": "^0.8.212",
    "graphql": "16.9.0",
    "graphql-ws": "5.16.0",
    "img-comparison-slider": "^8.0.6",
    "ionicons": "7.4.0",
    "jwt-decode": "4.0.0",
    "leaflet": "^1.9.4",
    "lodash-es": "4.17.21",
    "marked": "^15.0.0",
    "pixelmatch": "^6.0.0",
    "platform": "^1.3.6",
    "prettier": "^3.3.3",
    "rxjs": "7.8.1",
    "schema-dts": "^1.1.2",
    "tslib": "2.8.1",
    "typesafe-routes": "^12.3.0",
    "uuid": "^13.0.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/build": "20.1.6",
    "@angular/cli": "^20.3.2",
    "@angular/compiler": "^20.1.6",
    "@angular/compiler-cli": "^20.1.6",
    "@angular/language-service": "^20.1.6",
    "@badisi/wdio-harness": "^4.1.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-document-nodes": "^4.0.16",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@ionic/angular-toolkit": "^12.3.0",
    "@types/d3-scale": "^4.0.9",
    "@types/jest": "^29.5.14",
    "@types/js-cookie": "^3.0.4",
    "@types/leaflet": "^1.9.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.18.0",
    "@types/pixelmatch": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "fake-indexeddb": "^6.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^15.0.1",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.3.3",
    "sitemap": "^8.0.0",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  }
}
