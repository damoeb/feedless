{
  "name": "app",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "generate-verticals-data": "npx tsc generate-verticals-data.ts --outDir build/ && node build/generate-verticals-data.js",
    "prebuild": "yarn codegen",
    "prestart:feedless": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh feedless false",
    "start:feedless": "ng serve",
    "prestart:rss-proxy": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh rssProxy false",
    "start:rss-proxy": "ng serve",
    "prestart:page-change-tracker": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh pageChangeTracker false",
    "start:page-change-tracker": "ng serve",
    "prestart:untold-notes": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh untoldNotes true",
    "start:untold-notes": "ng serve",
    "prestart:reader": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh reader false",
    "start:reader": "ng serve",
    "prestart:upcoming": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh upcoming false",
    "start:upcoming": "ng serve",
    "start:upcoming:prod": "ng serve -c production",
    "prestart:visual-diff": "yarn codegen && dotenv -c .env -- sh generate-dev-app-config.sh visualDiff false",
    "start:visual-diff": "ng serve",
    "build": "ng build",
    "build:prod": "ng build -c production && yarn generate-verticals-data",
    "test": "ng test",
    "test:ci": "npx dotenv -e .env -- ng test -c ci",
    "lint": "yarn prettier --write --log-level=warn .",
    "outdated": "upgrade-interactive --latest",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "19.0.1",
    "@angular/common": "19.0.1",
    "@angular/core": "19.0.1",
    "@angular/forms": "19.0.1",
    "@angular/platform-browser": "19.0.1",
    "@angular/platform-browser-dynamic": "19.0.1",
    "@angular/router": "19.0.1",
    "@angular/service-worker": "19.0.1",
    "@apollo/client": "3.11.9",
    "@codemirror/autocomplete": "^6.12.0",
    "@codemirror/commands": "^6.3.3",
    "@codemirror/lang-css": "^6.2.1",
    "@codemirror/lang-html": "^6.4.7",
    "@codemirror/lang-javascript": "^6.2.1",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/lang-markdown": "^6.2.4",
    "@codemirror/language": "^6.10.0",
    "@codemirror/lint": "^6.4.2",
    "@codemirror/search": "^6.5.5",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.23.0",
    "@ionic/angular": "8.4.0",
    "@justinribeiro/lite-youtube": "1.5.0",
    "@ngx-translate/core": "^16.0.3",
    "@ngx-translate/http-loader": "^16.0.0",
    "d3-scale": "4.0.2",
    "dayjs": "1.11.13",
    "dexie": "^3.2.5",
    "flexsearch": "^0.7.43",
    "graphql": "16.9.0",
    "graphql-ws": "5.16.0",
    "img-comparison-slider": "^8.0.6",
    "ionicons": "7.4.0",
    "jwt-decode": "4.0.0",
    "leaflet": "^1.9.4",
    "lodash-es": "4.17.21",
    "marked": "^15.0.0",
    "pixelmatch": "^6.0.0",
    "platform": "^1.3.6",
    "prettier": "^3.3.3",
    "rxjs": "7.8.1",
    "schema-dts": "^1.1.2",
    "tslib": "2.8.1",
    "typesafe-routes": "^10.0.6",
    "uuid": "^11.0.3",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.2",
    "@angular-eslint/builder": "^18.4.0",
    "@angular-eslint/eslint-plugin": "^18.4.0",
    "@angular-eslint/eslint-plugin-template": "^18.4.0",
    "@angular-eslint/schematics": "^18.4.0",
    "@angular-eslint/template-parser": "^18.4.0",
    "@angular/cli": "^19.0.2",
    "@angular/compiler": "^19.0.1",
    "@angular/compiler-cli": "^19.0.1",
    "@angular/language-service": "^19.0.1",
    "@badisi/wdio-harness": "^4.1.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.1",
    "@graphql-codegen/typescript-document-nodes": "^4.0.11",
    "@graphql-codegen/typescript-operations": "^4.3.1",
    "@ionic/angular-toolkit": "^11.0.1",
    "@types/d3-scale": "^4.0.8",
    "@types/flexsearch": "^0.7.6",
    "@types/jasmine": "^5.1.4",
    "@types/jasminewd2": "^2.0.13",
    "@types/js-cookie": "^3.0.4",
    "@types/leaflet": "^1.9.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.18.0",
    "@types/pixelmatch": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.4.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "jasmine-core": "^5.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "prettier": "^3.3.3",
    "sitemap": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "5.6.3"
  }
}
